<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <template id="index">
        <t t-call ="website.layout">
            <t t-set="title">Academy</t>
            <div class="oe_structure">
                <t t-esc="user_id.name" />
                <t t-foreach="teachers" t-as="teacher">
                    <p><t t-esc="teacher.id"/> <t t-esc="teacher.name" /></p>
                </t>
            </div>
        </t>
    </template>
   
</odoo>
